{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceRoot}/frontend/src"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Jest All",
            "program": "frontend/node_modules/react-app-rewired/bin/index.js test --env=jsdom --script-version react-scripts-ts",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/frontend"

        }
    ]
}